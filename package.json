{
  "name": "whmcs-ispapi-helper",
  "description": "The WHMCS ISPAPI Helper Module.",
  "version": "0.0.0-development",
  "private": true,
  "author": {
    "name": "Kai Schwarz",
    "email": "kschwarz@hexonet.net"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.3.0"
  },
  "homepage": "https://github.com/hexonet/whmcs-ispapi-helper",
  "repository": "github:hexonet/whmcs-ispapi-helper.git",
  "bugs": "https://github.com/hexonet/whmcs-ispapi-helper/issues",
  "maintainers": [
    {
      "name": "Kai Schwarz",
      "url": "https://github.com/papakai"
    },
    {
      "name": "Anthony Schneider",
      "url": "https://github.com/anthonyschn"
    },
    {
      "name": "Tulasi Seelamkurthi",
      "url": "https://github.com/tulsi91"
    }
  ],
  "keywords": [
    "whmcs",
    "web host automation platform",
    "pricing importer addon",
    "price",
    "addon",
    "hexonet",
    "ispapi",
    "domain",
    "isp",
    "ssl",
    "cert",
    "dns",
    "backorder",
    "premium",
    "preregistration",
    "registration",
    "application"
  ],
  "devDependencies": {
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/commit-analyzer": "6.1.0",
    "@semantic-release/exec": "3.3.2",
    "@semantic-release/git": "7.0.8",
    "@semantic-release/github": "5.2.10",
    "conventional-changelog": "3.1.3",
    "semantic-release": "15.13.3"
  },
  "release": {
    "debug": false,
    "analyzeCommits": {
      "preset": "angular"
    },
    "generateNotes": {
      "preset": "angular"
    },
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/git",
      "@semantic-release/github"
    ],
    "prepare": [
      {
        "path": "@semantic-release/changelog",
        "changelogFile": "HISTORY.md"
      },
      {
        "path": "@semantic-release/git",
        "assets": [
          "HISTORY.md",
          "package.json",
          "composer.json"
        ],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "@semantic-release/github"
    ]
  },
  "scripts": {
    "semantic-release": "semantic-release"
  }
}
